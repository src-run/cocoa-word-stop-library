{
  "name": "src-run/cocoa-word-stop-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/cocoa-word-stop-library",
  "description": "Simple PHP-based word stemmer implementation using the Snowball algorithm.",
  "support": {
    "email": "rmf@src.run",
    "issues": "https://src.run/cocoa-word-stop-library/issues",
    "source": "https://src.run/cocoa-word-stop-library/git"
  },
  "keywords": [ "cocoa-word-stop-library", "cocoa", "stemmer", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "~7.1",
    "psr/cache": "~1.0",
    "symfony/polyfill-php72": "~1.5||dev-master"
  },
  "require-dev": {
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "~1.0",
    "phpunit/phpunit": "~5.7",
    "satooshi/php-coveralls": "~1.0",
    "sllh/php-cs-fixer-styleci-bridge": "~2.1",
    "symfony/cache": "~3.3"
  },
  "autoload": {
    "psr-4": {
      "SR\\Cocoa\\Word\\Stem\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Cocoa\\Word\\Stem\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
